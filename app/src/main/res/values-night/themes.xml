        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/second_color_30</item>
        <item name="colorPrimaryVariant">@color/teal_700</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_200</item>
        <item name="colorOnSecondary">@color/black</item>
        <item name="taskViewBottom">@color/taskListBottomColor</item>
        <item name="navigationBarColor">@color/navigationBarColor</item>
        <item name="imageTint">@color/imageTintColor</item>
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
    </style>
</resources>